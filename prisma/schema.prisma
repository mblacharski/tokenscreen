generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

model Token {
	id Int @id @default(autoincrement()) @map("token_id")
	name String @map("token_name")
	ticker String @map("token_ticker")
	description String @map("token_description") @default("")

	@@map("tokens")
}
